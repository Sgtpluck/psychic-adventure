<div class='row'>
  <div class='col-lg-2'>
    <h4>All Products</h4>
     <ul class='list-unstyled'>
      {{# each }}
      {{ this.id }}
      <li> {{# link-to "productEdit" this }}
        {{ name }} ${{ format-price price }} <br>
         <img {{ bind-attr src=avatar }} width="50px", height="50px;" class='thumbnail col-lg-16' />
         {{/ link-to }}
    
      </li>
      {{/ each }}
    </ul>
  </div>
  <div class='col-lg-10'>
   {{ outlet }}
  </div>
</div>